{
  "manifest_version": 2,
  "name": "Chrome Extension with notification",
  "description": "Chrome Extension with notification",
  "version": "1.0",
  "background": {
    "scripts": ["src/js/background.js"]
  },
  "content_scripts": [
    {
      "js": ["src/js/content.js"],
      "matches": ["*://example.com/*"]
    }
  ],
  "permissions": ["notifications"],
  "web_accessible_resources": ["resources/*"]
}
